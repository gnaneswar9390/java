package Basicss;
import java.util.Scanner;
public class fibonacci_series {
public static void main(String args[]) 
{
	int n;
	Scanner sc = new Scanner(System.in);
	n=sc.nextInt();
	fibo(n);
}
static void fibo(int n)
{
	int a=0,b=1,i;
	int c=a+b;
	System.out.println(a+""+b);
	for(i=2;i<=n;i++)
	{
		System.out.println(+c);
		a=b;
		b=c;
		c=a+b;
	}
}
}
