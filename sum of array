package gnani;
import java.util.*;
public class sum_of_array 
{

	public static void main(String[] args) 
	{
	
		int a[]=new int [10];
		int n,i;
		int sum=0;
		Scanner s =new Scanner(System.in);
		n=s.nextInt();
		
		System.out.println("enter n  values");
		for(i=0;i<n;i++)
		{
			a[i]=s.nextInt();
		}
		for(i=0;i<n;i++)
		{
		sum=sum+a[i];
		}
		System.out.println(sum);

	}

}
